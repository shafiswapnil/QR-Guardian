# PWA Development Environment Variables

# Enable PWA features in development
VITE_PWA_ENABLED=true

# Service Worker settings
VITE_SW_ENABLED=true
VITE_SW_SKIP_WAITING=true
VITE_SW_CLIENT_CLAIM=true

# Cache settings
VITE_CACHE_STATIC_ASSETS=true
VITE_CACHE_API_RESPONSES=true

# Development settings
VITE_PWA_DEV_MODE=true
VITE_PWA_LOG_LEVEL=info

# Lighthouse settings
VITE_LIGHTHOUSE_URL=http://localhost:4173
VITE_LIGHTHOUSE_OUTPUT=html